{
  "buildCommand": "python manage.py collectstatic --noinput",
  "builds": [
    {
      "src": "crop_project/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "crop_project/wsgi.py"
    }
  ]
}